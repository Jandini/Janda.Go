{
  "$schema": "http://json.schemastore.org/template",
  "author": "Matt Janda",
  "classifications": ["Common/Console/Go"],
  "name": "Console Go",
  "generatorVersions": "[1.0.0.0-*)",
  "description": "Console Go provides startup code for console application with dependency injection, logging, configuration and git versioning.",
  "groupIdentity": "go",
  "precedence": "4000",
  "identity": "Console.Go",
  "shortName": "consolego",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "ConsoleGo",
  "preferNameDirectory": true,
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            ".vs/**",
            ".git/**",
            "/**/obj/**",
            ".nuspec"
          ]
        },
        {
          "condition": "(!all)",
          "exclude": [
            "**/appsettings.json",
            "**/Extensions.cs",
            "**/Options.cs",
            "**/Settings.cs"
          ]
        }
      ]
    }
  ],
  "symbols": {
    "sourceName": {
      "type": "parameter",
      "replaces": "ConsoleGo"
    },
    "all": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false"
    }
  }
}
